---
import ArrowIcon from "../../icons/ArrowIcon.astro";
import BackHome from "../../icons/BackHome.astro";
import HeadingWithLine from "../../shared/components/HeadingWithLine/HeadingWithLine";

const { title, company } = Astro.props;
---

<section class="p-4">

    <h1 class="mt-12 text-rice text-5xl md:text-6xl font-bold">{title}</h1>

    <HeadingWithLine
        tag="h2"
        color="matcha"
        lineColor="wasabi"
        className=""
        animation="false"
        position="center">{company}</HeadingWithLine
    >
</section>
